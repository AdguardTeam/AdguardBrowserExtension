{
  "name": "browser-extension",
  "version": "5.0.63",
  "description": "AdGuard Extension",
  "scripts": {
    "dev": "cross-env BUILD_ENV=dev SWCRC= node -r @swc-node/register tools/bundle.ts",
    "dev:watch": "cross-env BUILD_ENV=dev node -r @swc-node/register -- tools/bundle.ts chrome --watch",
    "dev:watch:ff": "cross-env BUILD_ENV=dev node -r @swc-node/register -- tools/bundle.ts firefox --watch",
    "beta": "cross-env BUILD_ENV=beta node -r @swc-node/register tools/bundle.ts",
    "release": "cross-env BUILD_ENV=release node -r @swc-node/register tools/bundle.ts",
    "adguard-api": "cross-env BUILD_ENV=dev node -r @swc-node/register tools/bundle.ts adguard-api",
    "test": "MANIFEST_ENV=2 jest && MANIFEST_ENV=3 jest",
    "locales": "node -r @swc-node/register tools/locales.js",
    "resources": "node -r @swc-node/register tools/resources.ts",
    "resources:mv3": "yarn upgrade @adguard/dnr-rulesets && node -r @swc-node/register tools/resources-mv3.ts",
    "lint": "eslint --cache --max-warnings=0 --ext .jsx,.js,.tsx,.ts . && yarn check-types",
    "increment": "yarn version --patch --no-git-tag-version",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "check-types": "tsc --project tsconfig.with_types_mv2.json --noEmit && tsc --project tsconfig.with_types_mv3.json --noEmit",
    "disclaimer": "yarn lint | node -r @swc-node/register tools/insert-disclaimer.js",
    "copy-external": "tswebextension war Extension/web-accessible-resources",
    "convert-declarative": "node -r @swc-node/register tools/converter.ts"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:AdguardTeam/AdguardBrowserExtension.git"
  },
  "author": "adguard@adguard.com",
  "license": "GPL-3.0",
  "devDependencies": {
    "@adguard/dnr-rulesets": "^1.1.5",
    "@adguard/re2-wasm": "^1.2.0",
    "@sinonjs/fake-timers": "^10.0.2",
    "@swc-node/register": "^1.9.1",
    "@swc/core": "^1.5.24",
    "@swc/jest": "^0.2.36",
    "@types/chrome": "^0.0.268",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/css.escape": "^1.5.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.11",
    "@types/punycode": "^2.1.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-modal": "^3.16.3",
    "@types/sinon-chrome": "^2.2.11",
    "@types/ua-parser-js": "^0.7.37",
    "@types/webextension-polyfill": "^0.10.4",
    "@types/zip-webpack-plugin": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "axios": "^1.6.2",
