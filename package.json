    "@adguard/tswebextension": "https://github.com/AdguardTeam/tsurlfilter/raw/588b7b9e6dbb4e8b590a502033863a32f4422200/packages/tswebextension/tswebextension.tgz",
    "@xstate/react": "^4.1.1",
