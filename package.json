    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "filesize": "^10.1.2",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^7.0.1",
