        "postcss-preset-env": "10.1.2",
        "postcss-svg": "3.0.0",
        "preprocess-loader": "0.3.0",
        "rimraf": "5.0.10",
        "sinon": "19.0.2",
        "sinon-chrome": "3.0.1",
        "source-map-loader": "3.0.2",
        "style-loader": "3.3.2",
        "superjson": "2.2.1",
